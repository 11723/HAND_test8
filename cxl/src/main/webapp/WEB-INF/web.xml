<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>

  

  
  <servlet>
  <servlet-name>checkServlet</servlet-name>
  <servlet-class>com.hand.servlet.checkServlet</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>chakan</servlet-name>	
  	<servlet-class>com.hand.servlet.chakan</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>DeleteServlet</servlet-name>
  	<servlet-class>com.hand.servlet.DeleteServlet</servlet-class>
  </servlet>

  <servlet-mapping>
  <servlet-name>checkServlet</servlet-name>
  <url-pattern>/checkServlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>chakan</servlet-name>
  	<url-pattern>/chakan</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>DeleteServlet</servlet-name>
  	<url-pattern>/DeleteServlet</url-pattern>
  </servlet-mapping>
  
 
  
   <filter>
  <filter-name>encodingFilter</filter-name>
  <filter-class>com.hand.Onefilter.encodingFilter</filter-class>
  <init-param>
  <param-name>encoding</param-name>
  <param-value>utf-8</param-value>
  </init-param>
  </filter>


<filter-mapping>
	<filter-name>encodingFilter</filter-name>
	<url-pattern>/*</url-pattern>
</filter-mapping>

</web-app>
